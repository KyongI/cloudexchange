	$(NOECHO) $(NOOP)

bin/cfdefc :: bin/cfdefc.PL pm_to_blib
	$(PERLRUNINST) bin/cfdefc.PL bin/cfdefc


# --- MakeMaker installbin section:

EXE_FILES = bin/cfdefc

pure_all :: $(INST_SCRIPT)/cfdefc
	$(NOECHO) $(NOOP)

realclean ::
	$(RM_F) \
	  $(INST_SCRIPT)/cfdefc 

$(INST_SCRIPT)/cfdefc : bin/cfdefc $(FIRST_MAKEFILE) $(INST_SCRIPT)$(DFSEP).exists $(INST_BIN)$(DFSEP).exists
	$(NOECHO) $(RM_F) $(INST_SCRIPT)/cfdefc
