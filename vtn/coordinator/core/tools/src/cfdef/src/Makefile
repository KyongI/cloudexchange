	$(NOECHO) $(POD2MAN) --section=1 --perm_rw=$(PERM_RW) \
	  bin/cfdefc $(INST_MAN1DIR)/cfdefc.$(MAN1EXT) 
	$(NOECHO) $(POD2MAN) --section=3 --perm_rw=$(PERM_RW) \
	  lib/PFC/Conf/Block.pm $(INST_MAN3DIR)/PFC::Conf::Block.$(MAN3EXT) \
	  lib/PFC/Conf/Process.pm $(INST_MAN3DIR)/PFC::Conf::Process.$(MAN3EXT) \
	  lib/PFC/Conf/Map.pm $(INST_MAN3DIR)/PFC::Conf::Map.$(MAN3EXT) \
	  lib/PFC/Conf/Parameter/Integer.pm $(INST_MAN3DIR)/PFC::Conf::Parameter::Integer.$(MAN3EXT) \
	  lib/PFC/Conf/Lexer.pm $(INST_MAN3DIR)/PFC::Conf::Lexer.$(MAN3EXT) \
	  lib/PFC/Conf/Parser.pm $(INST_MAN3DIR)/PFC::Conf::Parser.$(MAN3EXT) \
	  lib/PFC/Conf/Token.pm $(INST_MAN3DIR)/PFC::Conf::Token.$(MAN3EXT) \
	  lib/PFC/Conf/Integer.pm $(INST_MAN3DIR)/PFC::Conf::Integer.$(MAN3EXT) \
	  lib/PFC/Conf.pm $(INST_MAN3DIR)/PFC::Conf.$(MAN3EXT) \
	  lib/PFC/Conf/DepFile.pm $(INST_MAN3DIR)/PFC::Conf::DepFile.$(MAN3EXT) \
	  lib/PFC/Conf/Constants.pm $(INST_MAN3DIR)/PFC::Conf::Constants.$(MAN3EXT) \
	  lib/PFC/Conf/Parameter.pm $(INST_MAN3DIR)/PFC::Conf::Parameter.$(MAN3EXT) \
	  lib/PFC/Conf/Parameter/Bool.pm $(INST_MAN3DIR)/PFC::Conf::Parameter::Bool.$(MAN3EXT) \
	  lib/PFC/Conf/Token/String.pm $(INST_MAN3DIR)/PFC::Conf::Token::String.$(MAN3EXT) \
	  lib/PFC/Conf/Token/Integer.pm $(INST_MAN3DIR)/PFC::Conf::Token::Integer.$(MAN3EXT) \
	  lib/PFC/Conf/Parameter/String.pm $(INST_MAN3DIR)/PFC::Conf::Parameter::String.$(MAN3EXT) 




# --- MakeMaker processPL section:

