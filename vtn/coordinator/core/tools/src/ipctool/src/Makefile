	bin/ipctc \
	bin/ipcsdump \
	lib/PFC/IPC/Parser.pm \
	lib/PFC/IPC/Field.pm \
	lib/PFC/IPC.pm \
	lib/PFC/IPC/StringTable.pm \
	lib/PFC/IPC/StructInfo/Generator.pm \
	lib/PFC/IPC/StructInfo.pm \
	lib/PFC/IPC/Struct.pm \
