	  bin/ipctc $(INST_MAN1DIR)/ipctc.$(MAN1EXT) \
	  bin/ipcsdump $(INST_MAN1DIR)/ipcsdump.$(MAN1EXT) 
	$(NOECHO) $(POD2MAN) --section=3 --perm_rw=$(PERM_RW) \
	  lib/PFC/IPC/Parser.pm $(INST_MAN3DIR)/PFC::IPC::Parser.$(MAN3EXT) \
	  lib/PFC/IPC/Field.pm $(INST_MAN3DIR)/PFC::IPC::Field.$(MAN3EXT) \
	  lib/PFC/IPC.pm $(INST_MAN3DIR)/PFC::IPC.$(MAN3EXT) \
